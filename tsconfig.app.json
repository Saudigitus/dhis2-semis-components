{
  "compilerOptions": {
  "baseUrl": ".",
  "target": "es5",
  "lib": [
  "es2021",
  "dom"
  ],
  "moduleResolution": "node",
  "paths": {
  "*": [
  "node_modules/*"
  ]
  },
  "declaration": true,
  "jsx": "react-jsx",
  "strict": false,
  "skipLibCheck": true,
  "esModuleInterop": true,
  "noImplicitAny": false,
  "outDir": "./dist",
  "allowJs": true,
  "rootDir": "./src",
  "declarationDir": "./dist/declarations",
  "resolveJsonModule": true
  },
  "exclude": [
  "node_modules",
  "src/stories",
  "vite.config.ts",
  "dist",
  "eslint.config.js",
  "eslint.config.d.ts"
  ]
  }